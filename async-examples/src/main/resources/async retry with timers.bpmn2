      <bpmn2:incoming>SequenceFlow_6</bpmn2:incoming>
      <bpmn2:outgoing>SequenceFlow_7</bpmn2:outgoing>
      <bpmn2:script>		
		System.out.println(&quot;check complete: &quot; + completionCheck);
		kcontext.setVariable(&quot;completionCheck&quot;, new Integer(completionCheck + 1));
		if (completionCheck == 2) {
			kcontext.setVariable(&quot;isComplete&quot;, new Boolean(true) );
		}
		System.out.println(&quot;is Complete? &quot; + isComplete);</bpmn2:script>
    </bpmn2:scriptTask>
